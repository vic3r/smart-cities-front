{
	"parser": "babel-eslint",
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"plugins": [
		"react",
		"jsx-a11y",
		"import"
	],
	"globals": {
		"React": true,
		"module": true,
		"process": false,
		"require": false,
		"console": false,
		"localStorage": false,
		"document": false,
		"HTMLAnchorElement": false,
		"global": false,
		"window": false
	},
	"rules": {
		"camelcase": "warn",
		"eqeqeq": "error",
		"no-eq-null": "warn",
		"indent": [
			"error",
			2
		],
		"no-use-before-define": "error",
		"new-cap": "error",
		"quotes": [
			"error",
			"single"
		],
		"no-console": "off",
		"no-trailing-spaces": "error",
		"no-undef": "error",
		"no-unused-vars": "warn",
		"max-len": [
			"error",
			100
		],
		"strict": "error",
		"comma-dangle": [
			"error",
			"always-multiline"
		],
		"semi": [
			"error",
			"never"
		],
		"react/prop-types": 0,
		"react/display-name": 0,
		"react/no-multi-comp": [
			1,
			{
				"ignoreStateless": true
			}
		],
		"react/jsx-pascal-case": 1,
		"react/jsx-closing-bracket-location": [
			1,
			{
				"location": "tag-aligned"
			}
		],
		"jsx-quotes": [
			"error",
			"prefer-double"
		],
		"react/jsx-boolean-value": 2,
		"jsx-a11y/aria-role": [
			2,
			{
				"ignoreNonDOM": true
			}
		],
		"react/jsx-curly-spacing": 2,
		"react/self-closing-comp": 2,
		"react/sort-comp": 2,
		"react/no-is-mounted": 2
	},
	"settings": {
		"react": {
			"version": "16.8.1"
		}
	}
}